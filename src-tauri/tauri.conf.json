{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "app-launcher",
  "version": "0.1.2",
  "identifier": "applauncher.io.github.swordtraveller",
  "build": {
    "beforeDevCommand": "pnpm dev",
    "devUrl": "http://localhost:14201",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "app-launcher",
        "width": 800,
        "height": 600,
        "transparent": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/icon.ico"
    ]
  }
}
